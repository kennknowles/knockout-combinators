Module generating
  type Value a
    generator :: Generator a
    value     :: a

  type Base <| Generator a
    size   :: Number
    next   :: () -> Value a
    shrink :: (a) -> Generator a

  -- ** Combinators
  as-generator :: Generator a -> Generator a

  choice    :: Generator a... -> Generator b
  frequency :: (Int, Generator a)... -> Generator b
  sequence  :: Generator a... -> Generator b

  size      :: Number -> Generator a -> Generator b
  transform :: (a -> b) -> Generator a -> Generator b
  label     :: String -> Generator a -> Generator a
  repeat    :: Generator a -> Generator b
